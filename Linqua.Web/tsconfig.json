{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "baseUrl": "./src",
    "paths": {},
    "lib": [
      "dom",
      "es6"
    ],
    "types": [
      "hammerjs",
      "jasmine",
      "node",
      "selenium-webdriver",
      "protractor",      
      "source-map",
      "uglify-js",
      "webpack"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  // "awesomeTypescriptLoaderOptions": {
  //   "forkChecker": true,
  //   "useWebpackText": true,
  //   "useCache": true
  // },
  "compileOnSave": false,
  "buildOnSave": false
}